    $DBConn_Pay->close();
    $DBConn->close();
    header("Location: " . $Redirect);
    unset($P_BANK_OPERATIONGrid);
    unset($P_FILTER_LISTSearch);
    unset($P_BANK_OPERATIONForm);
    unset($Tpl);
